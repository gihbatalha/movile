package hello;

public class Cancelamento {
	private String nomeOperadora = "";
	private int total = 0;
	private int voluntario = 0;
	private int involuntario = 0;
	
	private void addVoluntario(){
		voluntario++;
		total++;		
	}
	private void addInvoluntario(){
		involuntario++;
		total++;		
	}
	public int getTotal() {
		return total;
	}
	public int getVolutario() {
		return voluntario;
	}
	public int getInvoluntario() {
		return involuntario;
	}

	
	
}
