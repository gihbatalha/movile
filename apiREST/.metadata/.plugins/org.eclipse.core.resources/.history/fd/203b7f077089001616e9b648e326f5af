package hello;

public class Cancelamento {
	private int total = 0;
	private int volutario = 0;
	private int involuntario = 0;
	
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	public int getVolutario() {
		return volutario;
	}
	public void setVolutario(int volutario) {
		this.volutario = volutario;
	}
	public int getInvoluntario() {
		return involuntario;
	}
	public void setInvoluntario(int involuntario) {
		this.involuntario = involuntario;
	}
	
	

}
